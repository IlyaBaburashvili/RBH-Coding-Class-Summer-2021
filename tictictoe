            if square3=="-":
                square3="O"
                numberofmoves+=1
                currentmove="Player 1"
            else:
                print("This is not a legal move")
        elif player2move=="a":
            if square4=="-":
                square4="O"
                numberofmoves+=1
                currentmove="Player 1"
            else:
                print("This is not a legal move")
        elif player2move=="s":
            if square5=="-":
                square5="O"
                numberofmoves+=1
                currentmove="Player 1"
            else:
                print("This is not a legal move")
        elif player2move=="d":
            if square6=="-":
                square6="O"
